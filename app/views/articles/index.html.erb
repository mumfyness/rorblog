<h1>Photographer reviews</h1>
<div>
  <p><%= link_to 'Add Review', new_article_path %><p/>
</div>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th></th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
      <td><%= link_to ' Show', article_path(article) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Back', welcome_index_url %>
